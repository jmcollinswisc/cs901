	keywords = {consumer spending, Healthcare spending, payments, tax refunds},
	file = {Snapshot:/Users/madelinereed/Zotero/storage/2UWFICN4/papers.html:text/html;Submitted Version:/Users/madelinereed/Zotero/storage/74VMQMMS/Farrell et al. - 2018 - Deferred Care How Tax Refunds Enable Healthcare S.pdf:application/pdf}
